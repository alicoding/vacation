{
  "version": 1,
  "include": [
    "/*"
  ],
  "exclude": [
    "/_next/static/*",
    "/static/*",
    "/*.ico",
    "/*.png",
    "/*.svg",
    "/*.jpg",
    "/*.jpeg",
    "/*.gif",
    "/*.webp",
    "/robots.txt",
    "/sitemap*.xml",
    "/manifest.json"
  ],
  "proxies": {
    "/": {
      "origin": "https://vacation.alicoding.com"
    }
  },
  "rewrites": [
    { "source": "/((?!api|_next/static|_next/image|favicon.ico|public).*)", "destination": "/index.html" }
  ]
}